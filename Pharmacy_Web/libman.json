{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/lib/bootbox.js/"
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css",
        "animate.compat.min.css"
      ]
    },
    {
      "library": "sweetalert2@11.7.3",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "js/select2.min.js",
        "css/select2.min.css"
      ]
    },
    {
      "library": "selectize.js@0.15.2",
      "destination": "wwwroot/lib/selectize.js/",
      "files": [
        "js/selectize.min.js",
        "css/selectize.bootstrap5.min.css"
      ]
    }
  ]
}